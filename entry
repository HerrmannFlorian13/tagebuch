<?php
class entry{

	private entryID;
	private content;
	private userName;
	private visible;
	private entryPublic;
	
	public function entry(content, userName, visible, entryPublic){
		this.content = content;
		this.userName = userName;
		this.visible = visible;
		this.entryPublic = entryPublic;
	}

	private function setEntryID(entryID){
		this.entryID = entryID;
	}
	
	private function setViseble(visible){
		this.visible = visible;
	}
	
	private function setEntryPublic(entryPublic){
		this.entryPublic = entryPublic;
	}
	
	private function getContent(){
		return this.content;
	}
	
	private function getEntryID(){
		return this.entryID;
	}
	
	private function getUserName(){
		return this.userName;
	}
	
	private function getEntryPublic(){
		return this.entryPublic;
	}
	
	private function getVisible(){
		return this.visible;
	}
}
?>
